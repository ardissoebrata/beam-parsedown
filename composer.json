{
    "name": "ardissoebrata/beam-parsedown",
    "description": "More features for Parsedown.",
    "license": "MIT",
    "authors": [{
        "name": "Ardi Soebrata",
        "email": "ardissoebrata@gmail.com",
        "homepage": "https://mybeam.me"
    }],
    "homepage": "https://github.com/ardissoebrata/beam-parsedown",
    "keywords": ["ardissoebrata", "beam-parsedown", "markdown", "markdown extra", "parser", "parsedown", "Laravel"],
    "require": {
        "erusev/parsedown-extra": "^0.8.1",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "ArdiSSoebrata\\BeamParsedown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArdiSSoebrata\\BeamParsedown\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArdiSSoebrata\\BeamParsedown\\BeamParsedownServiceProvider"
            ],
            "aliases": {
                "BeamParsedown": "ArdiSSoebrata\\BeamParsedown\\Facades\\BeamParsedown"
            }
        }
    }
}